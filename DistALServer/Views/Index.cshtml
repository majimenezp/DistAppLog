@{
    Layout = "_Layout";
}

<table border="0" cellpadding="0" cellspacing="0">
    <thead>
        <tr>
            <th>Id</th>
            <th>Date</th>
            <th>App</th>
            <th>Module</th>
            <th>Level</th>
            <th>Message</th>
            <th>Exception</th>
        </tr>
    </thead>
    @foreach (var log in Model.Logs)
    { 
    <tbody>
        <tr>
            <td>@log.Id</td>
            <td>@log.Date</td>
            <td>@log.App</td>
            <td>@log.Module</td>
            <td>@log.Level</td>
            <td>@log.Message</td>
            <td>@log.Exception</td>            
        </tr>
    </tbody>
    }
</table>
